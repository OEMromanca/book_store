pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Stiahnutie zdrojového kódu z GitHub repozitára
                checkout([$class: 'GitSCM', branches: [[name: '*/gh-pages']], userRemoteConfigs: [[url: 'https://github.com/OEMromanca/book_store.git']]])
            }
        }
    }
}
